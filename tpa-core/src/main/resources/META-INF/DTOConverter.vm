import com.tongbanjie.commons.util.AbstractObjectConverter;
import org.springframework.beans.BeanUtils;
import org.springframework.stereotype.Component;

@Component
public class ${table.beanName}DTOConverter extends AbstractObjectConverter<${table.beanName}, ${table.beanName}DTO> {

    @Override
    protected ${table.beanName} onBuildDto(${table.beanName}DTO model) {
        ${table.beanName} domain = new ${table.beanName}();
        BeanUtils.copyProperties(model, domain);
        return domain;
    }

    @Override
    protected ${table.beanName}DTO onBuildModel(${table.beanName} domain) {
        ${table.beanName}DTO model = new ${table.beanName}DTO();
        BeanUtils.copyProperties(domain, model);
        return model;
    }
}
